/* components/search/index.wxss */
.container {
  background-color: #ffffff;
  position:fixed;
  height: 100rpx;
  border-top: 1px solid #f5f5f5;
  border-bottom: 1px solid #f5f5f5;
  display: flex;
  flex-direction: row;
  width: 750rpx;
  align-items: center;
  z-index:99;
  font-size: 30rpx;
}
.search-container {
  display: inline-flex;
  flex-direction: row;
  align-items: center;
  background-color: #f5f5f5;
  border-radius: 50px;
  margin-left: 20rpx;
}
.icon {
  width: 14px;
  height: 14px;
  margin-left: 12px;
  margin-right: 8px;
}
.in-bar {
  color: #999;
}
.bar {
  border-top-right-radius: 15px;
  border-bottom-right-radius: 15px;
  display: inline-block;
  height: 34px;
  width: 500rpx;
  font-size: 14px;
}
.cancel-img {
  width: 14px;
  height: 14px;
  margin-right: 10px;
}
.complete {
  line-height: 34px;
  width: 60px;
  text-align: center;
  display: inline-block;
  border: none;
}